                li
                    a.dropdown-toggle(href='/online/') Formación online
                li
                    a.dropdown-toggle(href='/congresos/') Congresos
                li
                    a.dropdown-toggle(href='/contacto/') Contacto
            form.navbar-form.navbar-form-cab.navbar-right
                .form-group
                    a#entrada.btn-u.btn-u-dark.btn-u-xs(href='https://poseidon.cfp.upv.es/portal-formacion/registro/identif.jsp?idioma=es') Registrarse
                    a#entrada.btn-u.btn-u-xs(href='https://poseidon.cfp.upv.es/portal-formacion/registro/identif.jsp?idioma=es')
